{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": false,
        "outDir": "dist",
        "noImplicitAny": true,
        "noImplicitReturns": true,
    },
    "typedocOptions": {
        "mode": "modules",
        "out": "docs"
    },
    "lib": [
        "es2015"
    ],
    "include": [
        "src/*"
    ]
}